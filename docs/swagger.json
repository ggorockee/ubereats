            "post": {
                "security": [
                    {
                        "Bearer": []
                    }
                ],
                "description": "로그인",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Auth"
                ],
                "summary": "로그인",
                "parameters": [
                    {
                        "description": "requestBody",
                        "name": "requestBody",
